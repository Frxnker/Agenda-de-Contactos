                <Button layoutX="549.0" layoutY="221.0" mnemonicParsing="false" text="Eliminar" />
                <Button layoutX="455.0" layoutY="221.0" mnemonicParsing="false" text="Recuperar" />
                <Label layoutX="455.0" layoutY="145.0" prefHeight="17.0" prefWidth="151.0" text="Elige el contacto, para" />
